<div class="flash_alert">
    <%= flash[:alert] %>
</div><br><br>

<div class="info">
  <p>Name: <%= @user.name %></p>
  <p>Tickets: <%= @user.tickets %></p>
  <p>Mood: <%= @user.mood %></p>
  <p>Height: <%= @user.height %></p>
  <% if @user.admin == true %>
  <p>ADMIN</p>
  <% end %>
  <%= link_to "See attractions", attractions_path %>
</div>